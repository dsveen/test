{
  "name": "nathanhoule.com",
  "private": true,
  "description": "Nathan Houle's personal website, found at nathanhoule.com.",
  "version": "0.0.0",
  "author": "Nathan Houle <nathan@nathanhoule.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndhoule/nathanhoule.com.git"
  },
  "packageManager": "yarn@3.3.1",
  "engines": {
    "node": ">= 20.0.0"
  },
  "scripts": {
    "build": "next build",
    "build-docker": "docker build -t ndhoule/nathanhoule.com:latest .",
    "clean": "rm -rf .eslintcache *.tsbuildinfo .next/",
    "deploy": "docker run --rm -v ./scripts:/data/scripts -v ./.nomad:/data/.nomad -w /data -e \"NOMAD_ADDR=${NOMAD_ADDR}\" -e \"IMAGE_TAG=${IMAGE_TAG}\" -e \"DRY_RUN=${DRY_RUN}\" -e 'NOMAD_SKIP_VERIFY=1' --entrypoint /bin/sh hashicorp/nomad scripts/deploy.sh",
    "dev": "next dev",
    "distclean": "yarn run clean && rm -rf dist/",
    "export": "next export -o dist/",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "start": "next start",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --incremental --watch"
  },
  "dependencies": {
    "@mapbox/maki": "^8.0.1",
    "@tanstack/react-query": "^4.29.7",
    "@tanstack/react-query-devtools": "^4.29.7",
    "@tmcw/togeojson": "^5.6.1",
    "@turf/turf": "^6.5.0",
    "@vanilla-extract/css": "^1.11.0",
    "@xmldom/xmldom": "^0.8.7",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "geojson": "^0.5.0",
    "mapbox-gl": "npm:empty-npm-package@1.0.0",
    "maplibre-gl": "^2.4.0",
    "moize": "^6.1.6",
    "ms": "^2.1.3",
    "next": "^13.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.0.23",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ndhoule/eslint-config": "8.0.0-1",
    "@next/eslint-plugin-next": "^13.4.3",
    "@tsconfig/next-strictest": "^1.0.1",
    "@types/eslint": "^8.37.0",
    "@types/geojson": "^7946.0.10",
    "@types/mapbox-gl": "^1",
    "@types/ms": "^0.7.31",
    "@types/node": "^20.2.3",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "~5.59.7",
    "@typescript-eslint/parser": "~5.59.7",
    "@vanilla-extract/next-plugin": "^2.1.2",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "~8.6.0",
    "eslint-config-problems": "~7.0.1",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jest": "~27.2.1",
    "eslint-plugin-jsx-a11y": "~6.7.1",
    "eslint-plugin-lodash": "~7.4.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-react": "~7.32.2",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-testing-library": "~5.9.1",
    "eslint-plugin-unicorn": "~47.0.0",
    "prettier": "2.8.8",
    "typescript": "^5.0.4"
  }
}
