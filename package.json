    "leaflet": "^1.9.3",
    "next": "^13.3.0",
    "node-html-parser": "^6.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ndhoule/eslint-config": "8.0.0-1",
    "@next/eslint-plugin-next": "^13.3.0",
    "@tsconfig/next-strictest": "^1.0.1",
    "@types/eslint": "^8.37.0",
    "@types/leaflet": "^1.9.3",
