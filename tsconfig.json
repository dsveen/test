{
  "extends": "@tsconfig/next-strictest/tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "@emotion/react",
    "noEmit": true,
    "plugins": [{ "name": "next" }],

    "importsNotUsedAsValues": "remove"
  },
  "include": ["next-env.d.ts", ".next/types/**/*.ts", "src/"],
  "exclude": ["node_modules"]
}
